{
    "name": "wonbin/laravel-mp-share",
    "description": "laravel miniprogram  share poster generator",
    "keywords":["Miniprogram","share","poster","wonbin"],
    "license": "MIT",
    "authors": [
        {
            "name": "wonbin wen",
            "email": "yuanbin975@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "verumconsilium/laravel-browsershot":"~1.1",
        "overtrue/laravel-filesystem-qiniu": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.8",
        "orchestra/database": "~3.8"
    },
    "autoload": {
        "psr-4": {
            "Wonbin\\Miniprogram\\Share\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wonbin\\Miniprogram\\Share\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wonbin\\Miniprogram\\Share\\MpSharePosterServiceProvider"
            ]
        }
    }
}
